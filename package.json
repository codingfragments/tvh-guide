		"svelte": "^3.53.1",
		"svelte-check": "^2.9.2",
		"svelte-loader": "^3.1.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^3.2.4",
		"vitest": "^0.25.3"
	},
	"type": "module",
	"dependencies": {
		"anylogger": "^1.0.11",
		"anylogger-console": "^1.0.0",
